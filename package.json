  },
  "bugs": {
    "url": "https://github.com/material-extended/mde/issues"
  },
  "keywords": [
    "angular",
    "angular popover",
    "material",
    "material design",
    "material extended",
    "material popover",
    "components",
    "popover"
  ],
  "engines": {
    "node": ">= 14.15.0",
    "npm": ">= 6.14.8"
  },
  "scripts": {
    "ng": "ng",
    "start": "npm run build:library && npm run serve",
    "serve": "ng serve",
    "build": "ng build",
    "build:library": "ng build @material-extended/mde",
    "build:library:prod": "ng build @material-extended/mde --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "release": "standard-version",
    "release:alpha": "standard-version --prerelease alpha",
    "release:beta": "standard-version --prerelease beta",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "14.2.12",
    "@angular/cdk": "14.2.7",
    "@angular/common": "14.2.12",
